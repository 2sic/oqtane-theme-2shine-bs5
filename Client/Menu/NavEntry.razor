@switch (ConfigName)
{
    case "Sidebar":
        <NavItemsSidebar PageNavigator="Start"></NavItemsSidebar>
        break;
    case "Mobile":
        <NavItemsMobile PageNavigator="Start"></NavItemsMobile>
        break;
