@inherits NavEntryBase
@*
File Purpose:       Add a menu based on the parameter 'ConfigName'
Recommendations:    -
Nice to know:       TODO:
*@
@switch (ConfigName)
{
    case "Sidebar":
        <NavItemsSidebar MenuBranch="@MenuTree"></NavItemsSidebar>
        break;
    case "Mobile":
        <NavItemsMobile MenuBranch="@MenuTree"></NavItemsMobile>
        break;
    case "Main": 
        <NavItems MenuBranch="@MenuTree"></NavItems>
        break;
    default: //Main
        <NavItems MenuBranch="@MenuTree"></NavItems>
        break;
}